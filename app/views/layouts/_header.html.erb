<header class= "row space-beetwen">
  <nav>
    <div class="logo">
      <%= link_to image_tag('logo'), root_path, alt:"logo" %>
    </div>
    <ul class="left">
      <li>
        <%= link_to "All Movies", movies_path %>
      </li>
    </ul>
    <ul class="right">
      <% if current_user %>
        <li><%= link_to current_user.name, current_user %></li>
        <li><%= link_to "Account Settings", edit_user_path(current_user)%></li>
        <li><%= link_to "Sign Out", session_path, method: :delete, class: "Button"%></li>
      <% else %>
        <li>
          <%= link_to "Sign Up", signup_path, class: "button" %>
        </li>
        <li><%= link_to "Sign In", signin_path, class: "button" %></li>
      <% end %>
    </ul>
  </nav>
</header>
